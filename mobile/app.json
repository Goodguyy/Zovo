{
  "expo": {
    "name": "Zovo",
    "slug": "zovo",
    "scheme": "vibecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibecode.zovo",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Zovo uses the camera to capture photos of your work before you publish them.",
        "NSPhotoLibraryUsageDescription": "Zovo accesses your photo library to let you choose media that appears in the app."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.vibecode.zovo"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./icon.png"
  }
}
